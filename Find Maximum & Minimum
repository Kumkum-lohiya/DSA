#include<iostream>
using namespace std;
int main()
{
	int n;
    cout<<"Enter the size of array :"<<endl;
	cin>>n;
	
	int arr[n];
	cout<<"Enter the elements of array :"<<endl;
	for(int i=0;i<n;i++)
	{
	  cin>>arr[i];	
	}	
	
	int max =INT_MIN;
	int min =INT_MAX;
	
	for(int i =0;i<n;i++)
	{
		if(arr[i]>max)
		{
			max = arr[i];
		}
		if(arr[i]<min)
		{
			min = arr[i];
		}
	}
     
     cout<<"Maximum element is "<<max<<endl;
     cout<<"Minimum element is "<<min;
    return 0;
}
		
