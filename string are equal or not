#include<iostream>
using namespace std;

int main()
{
	string str1,str2;
	int count = 0;
	cout<<"Enter the 1st string :";
	getline(cin,str1);
	
	cout<<"Enter the 2nd string :";
	getline(cin,str2);
	
	if(str1 == str2)
	{
		cout<<"The strings are equal "<<endl;
	}
	else cout<<"The strings are not equal "<<endl;
	
	
	return 0;
}
