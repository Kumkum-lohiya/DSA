#include<iostream>
#include<string>
using namespace std;
int main()
{
	string str,result = "";
	cout<<"Enter the string :"<<endl;
	getline(cin,str);
	
	for(int i=0;i<str.length();i++)
	{
		if(isalpha(str[i]))
		{
			result +=str[i];
		}
	}
	cout<<"After removing non-alphabet characters :"<<result<<endl;
	return 0;
}
