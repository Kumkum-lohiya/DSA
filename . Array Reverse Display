#include<iostream>
using namespace std;
class array{
	public:
	void insert(int arr[],int n)
{
	int i;
	for(i=0;i<n;i++)
	{
		cin>>arr[i];
	}
}
    void reverse(int arr[],int n)
    {
    	int i;
    	for(i=n-1;i>=0;i--)
    	{
    		cout<<arr[i]<<endl;
		}
	}
};
int main()
{
  int n;
  cout<<"Enter the size of an array :"<<endl;
  cin>>n;
   
   array a;
  int arr[n];
  cout<<"Enter the element of array :"<<endl;
  a.insert(arr,n);
  
  cout<<"Reverse of this array is :"<<endl;
  a.reverse(arr,n);
  
  return 0;
  	
}
