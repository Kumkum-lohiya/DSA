#include<iostream>
using namespace std;

int sumofnumber(int n)
{
    if(n == 1)
	return 1;	
    return n+ sumofnumber(n-1);
    
    
}
int main()
{
	int n;
	cout<<"Enter the number "<<endl;
	cin>>n;
	
	cout<<"\nSum of natural number from 1 to "<<n<<" are :\n";
	cout<<sumofnumber(n);
	
	return 0;
}
